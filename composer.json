{
    "name": "talliesoft/momolog",
    "description": "A simple PHP debugging utility that sends variables to a Node.js debug server",
    "type": "library",
    "keywords": ["debug", "logging", "development"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tallie Soft",
            "email": "talliesoft@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "suggest": {
        "ext-curl": "For better performance when sending debug data"
    },
    "autoload": {
        "files": [
            "momolog.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}